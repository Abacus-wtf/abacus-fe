    "@graphql-codegen/typescript-operations": "^2.3.5",
    "ethers": "^5.6.4",
    "graphql": "^16.2.0"
  },
  "devDependencies": {
    "dotenv": "^16.0.0",
    "rollup": "^2.64.0",
    "rollup-plugin-dts": "^4.1.0",
    "rollup-plugin-peer-deps-external": "^2.2.4",
    "rollup-plugin-typescript2": "^0.31.1",
    "typescript": "^3.8.3"
  }
